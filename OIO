#-------------------------------------------------QOL stuff starts----------------------------------------------------------------------------
@bot.event
async def on_ready():
    activity = discord.Game(name=f"support discord.gg/channels \n Cached Users: {len(bot.users)} Guilds: {len(bot.guilds)}", type=3)
    await bot.change_presence(status=discord.Status.idle, activity=activity)
    print("Bot is ready!")
